<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  >

  <browser:page
    for="*"
    name="display-delay.html"
    template="display-delay.pt"
    permission="zope.Public"
    />

  <browser:page
     for="*"
     name="launch-popup.html"
     template="launch-popup.pt"
     permission="zope.Public"
     />

  <browser:page
     for="*"
     name="popup.html"
     template="popup.pt"
     permission="zope.Public"
     />

  <browser:page
     for="*"
     name="alert.html"
     template="alert.pt"
     permission="zope.Public"
     />

  <browser:page
     for="*"
     name="divs.html"
     template="divs.pt"
     permission="zope.Public"
     />

</configure>
